>>> from client import GraphDatabase

>>> gdb = GraphDatabase("http://localhost:9999")

>>> gdb.node(name="Jimmy Wales", profession="Wikipedia Founder")

>>> nod1 = gdb.node[1]

>>> nod1 = gdb.node.get(1)

>>> nod2 = gdb.node(name="John Doe", profession="Hakcher")

>>> nod2["name"]
'John Doe'

>>> nod2["name"] = "John Does"

>>> nod1.KWONS(nod2)
'http://localhost:9999/relationship/0'
